{
	"name": "emmet",
	"displayName": "emmet",
	"description": "Emmet support for VS Code",
	"main": "./out/src/extension",
	"scripts": {
		"compile": "tsc -watch -p ./"
	},
	"contributes": {
		"configuration": {
			"properties": {
				"emmet.suggestAbbreviations": {
					"default": true,
					"description": "Shows possible emmet abbreviations as suggestions",
					"type": "boolean"
				},
				"emmet.suggestExpandedAbbreviation": {
					"default": true,
					"description": "Shows expanded emmet abbreviations as suggestions",
					"type": "boolean"
				}
			},
			"title": "Emmet configuration",
			"type": "object"
		}
	},
	"activationEvents": [
		"onLanguage:html",
		"onLanguage:jade",
		"onLanguage:slim",
		"onLanguage:haml",
		"onLanguage:xml",
		"onLanguage:xsl",
		"onLanguage:css",
		"onLanguage:scss",
		"onLanguage:sass",
		"onLanguage:less",
		"onLanguage:stylus",
		"onLanguage:javascriptreact",
		"onLanguage:typescriptreact"
	],
	"dependencies": {},
	"devDependencies": {
		"@types/node": "22.4.0"
	}
}
